{
    "version": 2,
    "builds": [
      {
        "src": "server.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "server.py"
      }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
  }
  